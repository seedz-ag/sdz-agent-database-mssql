SELECT 
       VE1_FILIAL,
       VE1_CODMAR,
       VE1_DESMAR,
       R_E_C_N_O_,
       D_E_L_E_T_ DELETADO_VE1,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(VE1_USERGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(VE1_USERGI, 16, 1)) - 50)), '19960101'), 101) AS DATA_INCLUSAO,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(VE1_USERGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(VE1_USERGA, 16, 1)) - 50)), '19960101'), 101) AS DATA_ALTERACAO
FROM VE1010 WITH (NOLOCK)
WHERE VE1_USERGI <> ' '
       AND (CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(VE1_USERGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(VE1_USERGI, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(VE1_USERGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(VE1_USERGI, 16, 1)) - 50)) <= '20211105'
       OR CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(VE1_USERGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(VE1_USERGA, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(VE1_USERGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(VE1_USERGA, 16, 1)) - 50)) <= '20211105')