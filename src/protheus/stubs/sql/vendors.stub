SELECT A3_FILIAL ,
       A3_COD ,
       A3_CGC ,
       A3_NOME ,
       R_E_C_N_O_ ,
       D_E_L_E_T_ DELETADO_SA3 ,
       TO_CHAR(TO_DATE('19960101') + ((ASCII(SUBSTR(A3_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(A3_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_INCLUSAO ,
       TO_CHAR(TO_DATE('19960101') + ((ASCII(SUBSTR(A3_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(A3_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_ALTERACAO
FROM SA3010
WHERE A3_USERLGI <> ' '
  AND TO_CHAR(TO_DATE('19960101') + ((ASCII(SUBSTR(A3_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(A3_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') >= '20210826'
  AND TO_CHAR(TO_DATE('19960101') + ((ASCII(SUBSTR(A3_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(A3_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') <= '20210827'