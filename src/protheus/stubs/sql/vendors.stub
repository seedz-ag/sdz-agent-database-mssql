SELECT A3_FILIAL,
       A3_COD,
       A3_CGC,
       A3_NOME,
       A3_TEL,
       A3_EMAIL,
       A3_END,
       A3_BAIRRO,
       A3_CEP,
       A3_MUN,
       A3_EST,
       A3_REGIAO,
       R_E_C_N_O_,
       D_E_L_E_T_ DELETADO_SA3,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(A3_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A3_USERLGI, 16, 1)) - 50)), '19960101'), 101) AS DATA_INCLUSAO,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(A3_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A3_USERLGA, 16, 1)) - 50)), '19960101'), 101) AS DATA_ALTERACAO
FROM SA3010 WITH (NOLOCK)
WHERE A3_USERLGI <> ' '
  AND (CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A3_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A3_USERLGI, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A3_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A3_USERLGI, 16, 1)) - 50)) <= '20211105'
       OR CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A3_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A3_USERLGA, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A3_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A3_USERLGA, 16, 1)) - 50)) <= '20211105')