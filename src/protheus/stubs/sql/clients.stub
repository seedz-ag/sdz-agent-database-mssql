SELECT A1_FILIAL ,
       D2_FILIAL ,
       (A1_COD || A1_LOJA) id ,
       A1_NOME ,
       A1_CGC ,
       A1_TEL ,
       A1_EMAIL ,
       B.R_E_C_N_O_ ,
       B.D_E_L_E_T_ DELETADO_SA1 ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.A1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.A1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_INCLUSAO ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.A1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.A1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_ALTERACAO
FROM SA1010 B
INNER JOIN SD2010 X ON X.D2_CLIENTE = B.A1_COD
AND X.D_E_L_E_T_ <> '*'
WHERE B.A1_USERLGI <> ' '
  AND (TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.A1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.A1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') >= '20211007'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.A1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.A1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') <= '20211008'
       OR TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.A1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.A1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') >= '20211007'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.A1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.A1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') <= '20211008')