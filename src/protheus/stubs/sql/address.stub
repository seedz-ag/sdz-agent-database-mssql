SELECT A1_FILIAL,
       CONCAT (A1_COD,
       A1_LOJA) id,
       A1_CGC,
       A1_CEP,
       A1_EST,
       A1_MUN,
       A1_BAIRRO,
       A1_END,
       A1_COMPLEM,
       A1_IBGE,
       R_E_C_N_O_,
       D_E_L_E_T_ DELETADO_SA1,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(A1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A1_USERLGI, 16, 1)) - 50)), '19960101'), 101) AS DATA_INCLUSAO,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(A1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A1_USERLGA, 16, 1)) - 50)), '19960101'), 101) AS DATA_ALTERACAO
FROM SA1010 WITH (NOLOCK)
WHERE A1_USERLGI <> ' '
       AND (CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A1_USERLGI, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A1_USERLGI, 16, 1)) - 50)) <= '20211105'
       OR CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A1_USERLGA, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(A1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(A1_USERLGA, 16, 1)) - 50)) <= '20211105')