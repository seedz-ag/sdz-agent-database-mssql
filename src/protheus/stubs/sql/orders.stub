SELECT 
      C5_FILIAL,
      A1_FILIAL,
      C6_FILIAL,
      CONCAT (C5_FILIAL, C5_CLIENTE, A1_CGC, C5_NUM, C5_TIPO) id,
      C5_EMISSAO,
      C5_STATUS,
      C5_CLIENTE,
      A1_CGC,
      C6_ENTREG,
      C5_TIPO,
      C5_VEND1,
      C5_VEND2,
      C5_VEND3,
      C5_DESC1,
      C5_CONDPAG,
      C5_PARC1,
      C5_DATA1,
      C5_PARC2,
      C5_DATA2,
      C5_PARC3,
      C5_DATA3,
      C5_PARC4,
      C5_DATA4,
      C5_FRETE,
      B.R_E_C_N_O_,
      B.D_E_L_E_T_ DELETADO_SC5,
      CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(B.C5_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C5_USERLGI, 16, 1)) - 50)), '19960101'), 101) AS DATA_INCLUSAO,
      CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(B.C5_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C5_USERLGA, 16, 1)) - 50)), '19960101'), 101) AS DATA_ALTERACAO
FROM SC5010 B WITH (NOLOCK)
INNER JOIN SA1010 X WITH (NOLOCK) ON X.A1_COD = B.C5_CLIENTE
AND X.D_E_L_E_T_ = ''
INNER JOIN SC6010 O WITH (NOLOCK) ON O.C6_NUM = B.C5_NUM
AND O.D_E_L_E_T_ = ''
WHERE B.C5_USERLGI <> ' '
      AND (CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C5_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C5_USERLGI, 16, 1)) - 50)) >= '20211101'
      AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C5_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C5_USERLGI, 16, 1)) - 50)) <= '20211105'
      OR CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C5_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C5_USERLGA, 16, 1)) - 50)) >= '20211101'
      AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C5_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C5_USERLGA, 16, 1)) - 50)) <= '20211105')