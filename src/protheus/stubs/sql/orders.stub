SELECT C5_FILIAL ,
       A1_FILIAL ,
       C6_FILIAL ,
       (C5_FILIAL || C5_CLIENTE || A1_CGC || C5_NUM || C5_TIPO) id ,
       C5_EMISSAO ,
       C5_STATUS ,
       C5_CLIENTE ,
       A1_CGC ,
       C5_VEND1 ,
       B.R_E_C_N_O_ ,
       B.D_E_L_E_T_ DELETADO_SC5 ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.C5_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.C5_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_INCLUSAO ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.C5_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.C5_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_ALTERACAO
FROM SC5010 B
INNER JOIN SA1010 X ON X.A1_COD = B.C5_CLIENTE
AND X.D_E_L_E_T_ <> '*'
INNER JOIN SC6010 E ON E.C6_NUM = B.C5_NUM
AND E.D_E_L_E_T_ <> '*'
WHERE B.C5_USERLGI <> ' '
  AND (TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.C5_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.C5_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') >= '20211007'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.C5_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.C5_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') <= '20211008'
       OR TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.C5_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.C5_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') >= '20211007'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.C5_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.C5_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') <= '20211008')