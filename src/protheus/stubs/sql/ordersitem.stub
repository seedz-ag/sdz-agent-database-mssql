SELECT 
       C6_FILIAL,
       C5_FILIAL,
       B1_FILIAL,
       CONCAT (C6_FILIAL, C6_CLI, C6_NUM, C6_ITEM) id,
       C6_ITEM,
       C6_NUM,
       C5_EMISSAO,
       C6_CLI,
       C6_PRODUTO,
       B1_CODITE,
       B1_DESC,
       C6_UM,
       C6_QTDVEN,
       C6_PRCVEN,
       C6_DESCONT,
       C6_VALDESC,
       C6_VALOR,
       C6_CHASSI,
       C6_TES,
       C6_QTDENT,
       B.R_E_C_N_O_,
       B.D_E_L_E_T_ DELETADO_SC6,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(B.C6_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C6_USERLGI, 16, 1)) - 50)), '19960101'), 101) AS DATA_INCLUSAO,
       CONVERT(VARCHAR, DATEADD(DAY, ((ASCII(SUBSTRING(B.C6_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C6_USERLGA, 16, 1)) - 50)), '19960101'), 101) AS DATA_ALTERACAO
FROM SC6010 B WITH (NOLOCK)
INNER JOIN SC5010 X WITH (NOLOCK) ON X.C5_NUM = B.C6_NUM
AND X.D_E_L_E_T_ = ''
INNER JOIN SB1010 V WITH (NOLOCK) ON V.B1_COD = B.C6_PRODUTO
AND V.D_E_L_E_T_ = ''
WHERE B.C6_USERLGI <> ' '
       AND (CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C6_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C6_USERLGI, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C6_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C6_USERLGI, 16, 1)) - 50)) <= '20211105'
       OR CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C6_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C6_USERLGA, 16, 1)) - 50)) >= '20211101'
       AND CONVERT(DATETIME, '1996-01-01', 105) + ((ASCII(SUBSTRING(B.C6_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTRING(B.C6_USERLGA, 16, 1)) - 50)) <= '20211105')