SELECT B1_FILIAL ,
       BM_FILIAL ,
       B1_COD ,
       B1_CODITE ,
       B1_DESC ,
       BM_CODMAR ,
       B1_UM ,
       B1_TIPO ,
       B1_PROC ,
       B.R_E_C_N_O_ ,
       B.D_E_L_E_T_ DELETADO_SB1 ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.B1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.B1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_INCLUSAO ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.B1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.B1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_ALTERACAO
FROM SB1010 B
INNER JOIN SBM010 X ON X.BM_GRUPO = B.B1_GRUPO
AND X.D_E_L_E_T_ <> '*'
WHERE B.B1_USERLGI <> ' '
  AND (TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.B1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.B1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') >= '20210826'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.B1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.B1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') <= '20210827'
       OR TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.B1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.B1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') >= '20210826'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.B1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.B1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') <= '20210827')